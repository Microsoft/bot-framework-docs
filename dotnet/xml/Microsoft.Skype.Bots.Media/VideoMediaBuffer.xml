    <Member MemberName="OriginalVideoFormat">
      <MemberSignature Language="C#" Value="public Microsoft.Skype.Bots.Media.VideoFormat OriginalVideoFormat { get; protected set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance class Microsoft.Skype.Bots.Media.VideoFormat OriginalVideoFormat" />
